    "test": "jest --coverage",
    "prepare": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "apollo-server": "^3.12.0",
    "graphql": "^16.6.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  }
}
